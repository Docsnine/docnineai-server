{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "config": {
                "runtime": "nodejs18.x",
                "maxDuration": 60
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ]
}